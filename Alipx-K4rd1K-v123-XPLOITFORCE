
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local UserId = LocalPlayer.UserId


local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "K4RD1K_V1_GUI"
ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling


local MainFrame = Instance.new("Frame")
MainFrame.Name = "MainFrame"
MainFrame.Parent = ScreenGui
MainFrame.Size = UDim2.new(0, 550, 0, 320)
MainFrame.Position = UDim2.new(0.25, 0, 0.25, 0)
MainFrame.BackgroundColor3 = Color3.fromRGB(240, 240, 240)
MainFrame.Active = true
MainFrame.Draggable = true
Instance.new("UICorner", MainFrame).CornerRadius = UDim.new(0, 16)
local UIStroke = Instance.new("UIStroke", MainFrame)
UIStroke.Color = Color3.fromRGB(200, 200, 200)
UIStroke.Thickness = 2


local Header = Instance.new("Frame")
Header.Parent = MainFrame
Header.Size = UDim2.new(1, 0, 0, 40)
Header.Position = UDim2.new(0, 0, 0, 0)
Header.BackgroundColor3 = Color3.fromRGB(220, 220, 220)
Header.BorderSizePixel = 0
Instance.new("UICorner", Header).CornerRadius = UDim.new(0, 12)

local TitleLabel = Instance.new("TextLabel")
TitleLabel.Parent = Header
TitleLabel.Text = "K4RD1K V1"
TitleLabel.Font = Enum.Font.SourceSansBold
TitleLabel.TextSize = 22
TitleLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TitleLabel.BackgroundTransparency = 1
TitleLabel.Position = UDim2.new(0, 10, 0, 5)
TitleLabel.Size = UDim2.new(0, 150, 1, -10)
TitleLabel.TextXAlignment = Enum.TextXAlignment.Left

local ThumbType = Enum.ThumbnailType.HeadShot
local ThumbSize = Enum.ThumbnailSize.Size100x100
local content, _ = Players:GetUserThumbnailAsync(UserId, ThumbType, ThumbSize)
local AvatarImage = Instance.new("ImageLabel")
AvatarImage.Parent = Header
AvatarImage.Image = content
AvatarImage.Size = UDim2.new(0, 30, 0, 30)
AvatarImage.Position = UDim2.new(1, -70, 0.5, -15)
AvatarImage.BackgroundTransparency = 1
Instance.new("UICorner", AvatarImage).CornerRadius = UDim.new(1, 0)

local CloseButton = Instance.new("TextButton")
CloseButton.Parent = Header
CloseButton.Text = "X"
CloseButton.Font = Enum.Font.SourceSansBold
CloseButton.TextSize = 18
CloseButton.TextColor3 = Color3.fromRGB(255, 0, 0)
CloseButton.BackgroundTransparency = 1
CloseButton.Position = UDim2.new(1, -30, 0, 5)
CloseButton.Size = UDim2.new(0, 25, 0, 25)
CloseButton.MouseButton1Click:Connect(function()
	ScreenGui:Destroy()
end)


local SideBar = Instance.new("ScrollingFrame")
SideBar.Name = "SideBar"
SideBar.Parent = MainFrame
SideBar.Size = UDim2.new(0, 80, 1, -40)
SideBar.Position = UDim2.new(0, 0, 0, 40)
SideBar.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
SideBar.ScrollBarThickness = 6
SideBar.CanvasSize = UDim2.new(0, 0, 0, 1000)
Instance.new("UICorner", SideBar).CornerRadius = UDim.new(0, 16)
local MenuLayout = Instance.new("UIListLayout", SideBar)
MenuLayout.SortOrder = Enum.SortOrder.LayoutOrder
MenuLayout.Padding = UDim.new(0, 5)


local ContentFrame = Instance.new("ScrollingFrame")
ContentFrame.Name = "ContentFrame"
ContentFrame.Parent = MainFrame
ContentFrame.Size = UDim2.new(1, -80, 1, -40)
ContentFrame.Position = UDim2.new(0, 80, 0, 40)
ContentFrame.CanvasSize = UDim2.new(0, 0, 0, 1000)
ContentFrame.ScrollBarThickness = 6
ContentFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Instance.new("UICorner", ContentFrame).CornerRadius = UDim.new(0, 12)


local function clearContent()
	for _, v in ipairs(ContentFrame:GetChildren()) do
		if not v:IsA("UIListLayout") and not v:IsA("UICorner") then
			v:Destroy()
		end
	end
end

-- Home Menu
local function openHome()
	clearContent()
	local welcome = Instance.new("TextLabel")
	welcome.Text = "WELCOME to SCRIPT k4RD1K v1\nOwned by ALIPX\nTeaM : XF - XPLOIT FORCE"
	welcome.Size = UDim2.new(1, -20, 0, 120)
	welcome.Position = UDim2.new(0, 10, 0, 10)
	welcome.TextColor3 = Color3.fromRGB(0, 0, 0)
	welcome.Font = Enum.Font.SourceSansBold
	welcome.TextSize = 20
	welcome.TextWrapped = true
	welcome.BackgroundTransparency = 1
	welcome.Parent = ContentFrame
end


local function addScriptButtons(data)
	for _, info in ipairs(data) do
		local btn = Instance.new("TextButton")
		btn.Text = info[1]
		btn.Size = UDim2.new(1, -20, 0, 40)
		btn.Position = UDim2.new(0, 10, 0, 0)
		btn.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
		btn.TextColor3 = Color3.fromRGB(255, 255, 255)
		btn.Font = Enum.Font.SourceSansBold
		btn.TextSize = 18
		btn.Parent = ContentFrame
		Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 8)
		btn.MouseButton1Click:Connect(function()
			loadstring(game:HttpGet(info[2]))()
		end)
	end
end


local function openTPMenu()
	clearContent()
	addScriptButtons({
		{"Teleport Train", "https://raw.githubusercontent.com/ringtaa/NEWTPTRAIN.github.io/refs/heads/main/TRAIN.LUA"},
		{"Teleport Sterling", "https://raw.githubusercontent.com/ringtaa/sterlingnotifcation.github.io/refs/heads/main/Sterling.lua"},
		{"Teleport Tesla", "https://raw.githubusercontent.com/ringtaa/tptotesla.github.io/refs/heads/main/Tptotesla.lua"},
		{"Teleport Castle", "https://raw.githubusercontent.com/ringtaa/castletpfast.github.io/refs/heads/main/FASTCASTLE.lua"},
		{"Teleport Cannon", "https://raw.githubusercontent.com/ringtaa/Tpfort.github.io/refs/heads/main/Tpfort.lua"},
		{"Teleport to End [Outlaw Chase]", "https://raw.githubusercontent.com/hbjrev/tpend.github.io/refs/heads/main/ringta.lua"},
		{"TP STILLWATER AND AUTO SWING", "https://raw.githubusercontent.com/m00ndiety/Stillwater/refs/heads/main/obf_Prisonteleport.lua.txt"}
	})
end

